{
  "manifest_version": 3,
  "name": "Genni - Analytics mocking tool",
  "version": "1.4.5",
  "description": "A tool to help manage Analytics mocks",
  "action": {
    "default_popup": "index.html",
    "default_title": "Genni - Analytics mocking tool"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
